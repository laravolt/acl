{
    "name": "laravolt/acl",
    "description": "Basic Laravel ACL",
    "keywords": [
        "laravolt",
        "acl"
    ],
    "homepage": "https://github.com/laravolt/acl",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "bayu.hendra@javan.co.id",
            "homepage": "https://laravolt.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.2",
        "illuminate/support": "~5.6|^6.0",
        "illuminate/database": "~5.6|^6.0",
        "spatie/once": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Acl\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Acl\\ServiceProvider"
            ],
            "aliases": {
                "Acl": "Laravolt\\Acl\\Facade"
            }
        }
    }
}
